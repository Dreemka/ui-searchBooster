{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": [
      "standard-with-typescript",
      "plugin:vue/vue3-essential"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "project": "./tsconfig.json",
      "extraFileExtensions": [".vue"]
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "vue",
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/dot-notation": "error"
    }
  }